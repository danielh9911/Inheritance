package tutorial;

public class _1_14_Inheritance extends _1_13_Dog {

	private int food;
	
	public _1_14_Inheritance(String name, int age, int food) {
		super(name, age);
		this.food = food;
	}
	
	public void speak() {
		System.out.println("Meow my name is " + this.name + " I am " + this.age + " and I eat " + this.food + " grams of food");
	}
	
}

